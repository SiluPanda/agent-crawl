{
  "name": "agent-crawl",
  "version": "3.0.0",
  "description": "High-performance, lightweight TypeScript web scraping library for LLM Agents.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "tsx test/demo.ts",
    "test:unit": "node --import tsx --test test/*.test.ts",
    "test:perf": "tsx test/demo_perf.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "scraper",
    "crawler",
    "llm",
    "agent",
    "ai",
    "playwright",
    "typescript",
    "markdown"
  ],
  "author": "silupanda",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silupanda/agent-crawl.git"
  },
  "bugs": {
    "url": "https://github.com/silupanda/agent-crawl/issues"
  },
  "homepage": "https://github.com/silupanda/agent-crawl#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/node": "^25.0.9",
    "playwright": "^1.57.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.1.12"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "node-html-markdown": "^2.0.0",
    "playwright-core": "^1.57.0"
  },
  "peerDependencies": {
    "zod": "^4.0.0"
  }
}
